#VRML V2.0 utf8


DEF	nivel Group	{
	children [
		DEF medionivel Group {
			children [
				Transform {
					translation	0 0 3.5
					children DEF octavo Transform {
						scale 0.4 0.4 0.4  
						children [
							#Columna
							Transform {			 
								translation	-3.5 0 0
								children Inline	{
									url	"../../VRML/cursoVRML/piezas/columna.wrl"
									bboxCenter 0 0 0
									bboxSize -1 -1 -1
								}
							}
							#Arco
							Transform {
								scale 1 1 0.7
								translation	0 4 0
								children Inline	{
									url	"../../VRML/cursoVRML/piezas/arco.wrl"
									bboxCenter 0 0 0
									bboxSize -1 -1 -1
								}
							}
						]
					}
				}
				Transform {			   
					rotation 0 1 0 0.785
					children Transform {
						translation	0 0 3.5	 
						children USE octavo
					}
				}
				Transform {			   
					rotation 0 1 0 1.57
					children Transform {
						translation	0 0 3.5	 
						children USE octavo
					}
				}
				Transform {			   
					rotation 0 1 0 2.355
					children Transform {
						translation	0 0 3.5	 
						children USE octavo
					}
				}
			]
		}
		Transform {
			rotation 0 1 0 3.14
			children USE medionivel
		}
	]
}
Transform {
	scale 0.95 0.95 0.95
	rotation 0 1 0 0.392
	translation	0 3.12 0
	children USE nivel
}
  
