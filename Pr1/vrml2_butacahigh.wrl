#VRML V2.0 utf8

#------------------------
# ButacaMed
#------------------------
Inline {
	url	"vrml2_butacamed.wrl"
	bboxCenter 0 0 0
	bboxSize -1 -1 -1
}

#------------------------
# Colgador
#------------------------
Transform {
	scale 1 .9 1	 
	rotation 0 0 1 1.8
	translation	-1.51 2.82 0 
	children [
		Transform {
			scale .1 .3 .1
			translation	-0.2 0.3 -0.75
			children Shape {
				geometry Cylinder {
					height 1
					radius 0.3
				}
				appearance Appearance {
					material Material {
						diffuseColor .3 .3 .3
						specularColor .5 .5 .5
						emissiveColor .15 .15 .15
						ambientIntensity 0
					}
				}
			}
		}
	]
} 